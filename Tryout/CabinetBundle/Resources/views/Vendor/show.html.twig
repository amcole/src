			</div>
			{% endblock %}
			{# <!-- END PAGE HEADER--> #}
			{# <!-- BEGIN PAGE CONTENT--> #}
			{% block page_content %} 
			<div class="row">
				<div class="col-md-12">
					<div class="note note-success">
						<h4 class="block">Vendor</h4>
						<p>
						</p>
					</div>
					<!-- BEGIN PORTLET-->
				    <table class="record_properties">
				        <tbody>
				            <tr>
				                <th>Id</th>
				                <td>{{ entity.id }}</td>
				            </tr>
				            <tr>
				                <th>Vendorname</th>
				                <td>{{ entity.vendorName }}</td>
        {% for quickProfile in quickProfiles %}
            <tr>
                <td >{{ quickProfile.id }}</td>
                <td>
                </td>
            </tr>
        {% endfor %}                
				            </tr>
				        </tbody>
				    </table>
					<ul class="record_actions">
					    <li>
					        <a href="{{ path('vendor') }}">
					            Back to the list
					        </a>
					    </li>
					    <li>
					        <a href="{{ path('vendor_edit', { 'slug': entity.slug }) }}">
					            Edit
					        </a>
					    </li>
					    <li>{{ form(delete_form) }}</li>
					</ul>
					<!-- END PORTLET-->
				</div>
			</div>
			{% endblock %}
			{# <!-- END PAGE CONTENT--> #} 			

