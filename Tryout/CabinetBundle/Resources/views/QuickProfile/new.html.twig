{% extends '::base.html.twig' %}

{% block body -%}
    <h1>QuickProfile creation</h1>

	<form action="{{ path('quickprofile_create') }}" method="POST">
        {{ form_errors(form) }}
    

	{{ form_row(form.vendorMultiLocation) }}
    {{ form_row(form.vendorIndustry) }}
    {{ form_row(form.publicCompany) }}
    {{ form_row(form.numLevelsToOwner) }}
    {{ form_row(form.descCompanyCulture) }}
    {{ form_row(form.peerlessService) }}
    {{ form_row(form.client) }}
    
        {{ form_rest(form) }}
    

    <input type="submit" value="Register!" />
    </form>
    <li>
        <a href="{{ path('quickprofile') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
